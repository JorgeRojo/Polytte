#include <Arduino.h>
#include "./Storage.h"

void setup()
{
    Serial.begin(115200);
    delay(1000);

    Serial.println("--------------------");

    storage.setup();

    Storage_t data = storage.getData();

    if (data.wifiPort != 22)
    {
        Serial.printf(">>>----->>> saveWifi \n");
        storage.saveWifi("wifi", "pass", "host", 22);
    }

    storage.print();

    Serial.println("--------------------");
}

void loop()
{
}